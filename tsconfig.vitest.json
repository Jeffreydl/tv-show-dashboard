{
  "extends": "./tsconfig.app.json",
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "src/**/*.spec.ts",
    "src/**/*.test.ts",
    "src/**/__tests__/*",
    "env.d.ts"
  ],
  "exclude": [],
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.vitest.tsbuildinfo",
    "lib": ["esnext", "dom"],
    "types": ["vitest/globals", "node", "jsdom"]
  }
}
